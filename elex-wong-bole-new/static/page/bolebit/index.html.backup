  function getList(list) {
    list.forEach(function (item, index) {
      var gameItem = '';
      gameItem = '<div class="game-item"><div class="game-item-img"><img src="' + item.img + '"></div>';
      if (item.hot)
        gameItem += '<div class="game-item-hot"><img src="assets/images/hot_lable.png"></div>';
      gameItem += '<div class="game-item-text"><div class="game-item-title" data-translate="' + item.id + '_name" >' + item.name + '</div><div class="game-item-desc" data-translate="' + item.id + '_desc">' + item.desc + '</div><div data-translate="details" class="game-item-button2" onClick="openpop(`' + item.id + '`)">' + dictionary[lang]['details'] + '</div><div data-translate="tryNow" class="game-item-button" onClick="openlink(`' + item.demo + '`)">' + dictionary[lang]['tryNow'] + '</div></div></div>';
      $('.game-list').append(gameItem);
    });

    $('.gameTotal').html(list.length);

    // mobile load more button
    currentItems = 3;
    if (list.length > 3) {
      $('#load').css('display', 'block');
    } else {
      $('#load').css('display', 'none');
    }

    // coming soon
    if (list.length < 1) {
      $('.coming-soon').css('display', 'block');
    } else {
      $('.coming-soon').css('display', 'none');
    }
  }

  getList(gameList);

  function handleGameNav(iconId, name) {
    if (window.innerWidth >= 768) {
      window.scrollTo({ left: 0, top: 312, behavior: 'smooth' })
    } else {
      window.scrollTo({ left: 0, top: 0, behavior: 'smooth' });
    }

    // change active class
    $(".game-nav-item").removeClass("active");
    $(".game-nav-item:nth-child(" + iconId + ")").addClass("active");

    // change gamenav icon
    for (var i = 1; i < 6; i++) {
      if (i == iconId) {
        $(".icon" + iconId).attr("src", "assets/images/icon/icon" + iconId + "_s.png");
      } else {
        $(".icon" + i).attr("src", "assets/images/icon/icon" + i + "_d.png");
      }
    }

    // remove game item
    $('.game-item').remove();

    // filter the gamelist and append
    switch (iconId) {
      case 1:
        getList(gameList);
        break;
      case 2:
      case 3:
      case 4:
        var newGameList = gameList.filter(function (item) {
          return item.type == name;
        });
        getList(newGameList);
        break;
      default:
        getList(gameList);
        break;
    }
  }

  // mobile load more button
  const loadmoreBtn = document.querySelector('#load');
  loadmoreBtn.addEventListener('click', (e) => {
    loadmore();
  });

  function loadmore() {
    const elementList = Array.from(document.querySelectorAll('.mobile .game-item'));
    for (let i = currentItems; i < currentItems + 3; i++) {
      if (elementList[i]) {
        elementList[i].style.display = 'block';
      }
    }
    currentItems += 3;
    if (currentItems >= elementList.length) {
      loadmoreBtn.style.display = 'none';
    }
  }

  //carousel
  $('.carousel').carousel({
    interval: 8000,
  });

  $(document).ready(function () {
    $(window).scroll(function () {
      const elementList = Array.from(document.querySelectorAll('.mobile .game-item'));
      var notEndOfItem = currentItems < elementList.length;
      var reachBottom = $(window).scrollTop() + $(window).innerHeight() >= ($(document).height() - 1);
      if (notEndOfItem && reachBottom) {
        loadmore();
      }
    });
  });


  var isShowContactNav = false;
  function handleContactNav() {
    isShowContactNav = !isShowContactNav;
    if (isShowContactNav) {
      $('.contact-slide').show();
      $('.contact-slide').addClass('slide-in');
      var listener1 = $('.contact-slide').one('animationend', function () {
        $('.contact-slide').removeClass('slide-in');
      })
    } else {
      $('.contact-slide').addClass('slide-out');
      var listener1 = $('.contact-slide').one('animationend', function () {
        $('.contact-slide').removeClass('slide-out');
        $('.contact-slide').hide();
      })
    }
  };
  function openlink(link) {
    window.open(link);
  }

  function openpop(id) {
    var tempgame = gameList.find(x => x.id === id);
    var temp = '<div class="popheader"><div data-translate="game_info">' + dictionary[lang]['game_info'] + '</div><button type="button" class="close" aria-label="Close" onclick="closepop()"><span aria-hidden="true">&times;</span></button></div>' +
      '<div class="popcontent1"><div class="popitem"><div class="poptitle" data-translate="game_name">' + dictionary[lang]['game_name'] + '</div><div data-translate="' + tempgame.id + '_name">' + tempgame.name + '</div></div><hr/>' +
      '<div class="popitem"><div class="poptitle" data-translate="game_type">' + dictionary[lang]['game_type'] + '</div><div data-translate="';
    if (tempgame.type == 'slot') {
      temp += 'slot_machine">Slot Machine';
    }
    else if (tempgame.type == "fishing") {
      temp += 'fishing">Fishing';
    }
    else {
      temp += 'poker">Poker';
    }

    temp += '</div></div><hr/>';
    if (tempgame.reels != null)
      temp += '<div class="popitem"><div class="poptitle" data-translate="reels">' + dictionary[lang]['reels'] + '</div><div>' + tempgame.reels + '</div></div><hr/>';
    if (tempgame.lines != null)
      temp += '<div class="popitem"><div class="poptitle" data-translate="lines">' + dictionary[lang]['lines'] + '</div><div>' + tempgame.lines + '</div></div><hr/>';
    if (tempgame.rtp != null)
      temp += '<div class="popitem"><div class="poptitle" data-translate="rtp">' + dictionary[lang]['rtp'] + '</div><div>' + tempgame.rtp + '</div></div><hr/>';
    temp += '<div class="popitem"><div class="poptitle" data-translate="max_win">' + dictionary[lang]['max_win'] + '</div><div data-translate="lines">100x bet</div></div><hr/>';
    if (tempgame.gamble_mode != null)
      temp += '<div class="popitem"> <div class="poptitle" data-translate="game_features">' + dictionary[lang]['game_features'] + '</div> <div>' + tempgame.gamble_mode + '</div></div><hr/>';

    temp += '</div><div class="popcontent2"><div class="popitem2"><div class="poptitle" data-translate="support_platform">' + dictionary[lang]['supported_platform'] + '</div>' +
      '<img src="assets/images/platform/computer.png" class="small_icon" alt=""><img src="assets/images/platform/laptop.png" class="small_icon" alt=""><img src="assets/images/platform/mobile.png" class="small_icon" alt=""><img src="assets/images/platform/tablet.png" class="small_icon" alt="">' +
      '</div><hr class="mobile">' +
      '<div class="popitem2"><div class="poptitle" data-translate="os">' + dictionary[lang]['supported_os'] + '</div><img src="assets/images/os/windows.png" class="small_icon" alt=""><img src="assets/images/os/html5.png" class="small_icon" alt=""><img src="assets/images/os/apple.png" class="small_icon" alt=""><img src="assets/images/os/android.png" class="small_icon" alt=""><img src="assets/images/os/ios.png" class="small_icon" alt="">' +
      '</div><hr class="mobile">' +
      '<div class="popitem2"><div class="poptitle" data-translate="language">' + dictionary[lang]['language'] + '</div><img src="assets/images/flag/england.png" class="lang_icon" alt=""><img src="assets/images/flag/china.png" class="lang_icon" alt=""><img src="assets/images/flag/vietnam.png" class="lang_icon" alt=""><img src="assets/images/flag/korea.png" class="lang_icon" alt="">' +
      '</div></div>' +
      '<div class="popcontent3"><img src="' + tempgame.img + '" alt=""><div data-translate="tryNow" class="pop-button" onClick="openlink(`' + tempgame.demo + '`)">' + dictionary[lang]['tryNow'] + '</div></div>';

    $('.popwrap').html(temp);
    $('.popup').show();
    $('div.mobile').css('position', 'fixed');
  }

  function closepop() {
    $('.popup').hide();
    $('div.mobile').css('position', '');
  }
