@keyframes living {
  0% {
    transform: scale(0.5);
    opacity: 0.5;
  }

  50% {
    transform: scale(1.5);
    opacity: 0.2;
  }

  100% {
    transform: scale(0.5);
    opacity: 0.5;
  }
}

/* clue-text-2-effect */
@keyframes clue-text-2-effect {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
/* clue-text-2-effect */

/* highlight-effect */
@keyframes highlight-effect {
  0% {
    width: 0%;
  }
  100% {
    width: 100%;
  }
}
@-webkit-keyframes highlight-effect {
  /* Chrome, Safari */
  0% {
    width: 0%;
  }
  100% {
    width: 100%;
  }
}
@-moz-keyframes highlight-effect {
  /* FF */
  0% {
    width: 0%;
  }
  100% {
    width: 100%;
  }
}
@-ms-keyframes highlight-effect {
  /* IE10 */
  0% {
    width: 0%;
  }
  100% {
    width: 100%;
  }
}
@-o-keyframes highlight-effect {
  /* Opera */
  0% {
    width: 0%;
  }
  100% {
    width: 100%;
  }
}
/* highlight-effect */
