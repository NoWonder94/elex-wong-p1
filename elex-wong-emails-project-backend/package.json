{
  "name": "emails-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "prisma-gen": "cd prisma/main-schema && npx prisma generate",
    "prisma-deploy": "cd prisma/main-schema && npx prisma migrate deploy",
    "prisma-gen-legacy": "cd prisma/legacy-schema && npx prisma generate",
    "gen-env": "ts-node environment/set_env.ts",
    "prepare": "npm run gen-env && npm run prisma-gen-legacy && npm run prisma-gen",
    "start": "npm run prepare && nodemon --watch src --watch ts-node --files src/app.ts",
    "admin": "npm run prepare && nodemon --watch src --watch ts-node --files src/admin/app.ts",
    "start-staging": "npm run prepare && pm2 start --name emails-com-backend-staging ts-node -- --files src/app.ts",
    "start-prod": "npm run prepare && pm2 start --name emails-com-backend-prod  ts-node -- --files src/app.ts",
    "stop-staging": "pm2 delete emails-com-backend-staging",
    "stop-prod": "pm2 delete emails-com-backend-prod",
    "migrate-old-db": "ts-node --files src/migrations/old_db_to_first_prisma.ts",
    "reset-database": "cd prisma/main-schema && npx prisma generate && npx prisma db push"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
