    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.4.7",
    "moment": "^2.29.3",
    "mongoose": "^6.3.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
